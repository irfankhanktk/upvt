{
  "name": "RN_WITBIT",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "install-apk": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint src/** -f node_modules/eslint-detailed-reporter/lib/detailed.js -o report.html",
    "pod-install": "cd ios && pod install",
    "debug": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "jsbundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "rm-duplicate-drawables": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/*",
    "rm-duplicate": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "clean": "cd android && ./gradlew clean",
    "apk-debug": "cd android && ./gradlew assembleDebug -x bundleDebugJsAndAssets",
    "apk-release": "cd android && ./gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "release": "cd android && ./gradlew :app:assembleRelease",
    "apk-release-app": "cd android && ./gradlew :app:assembleRelease -x bundleReleaseJsAndAssets",
    "bundle": "cd android && ./gradlew bundleRelease -x bundleReleaseJsAndAssets"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-firebase/app": "^15.6.0",
    "@react-native-firebase/auth": "^15.6.0",
    "@react-native-firebase/firestore": "^15.6.0",
    "@react-native-firebase/messaging": "^15.6.0",
    "@react-native-firebase/storage": "^15.6.0",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@react-navigation/stack": "^6.3.1",
    "@reduxjs/toolkit": "^1.8.5",
    "i18next": "^21.9.2",
    "moment": "^2.29.4",
    "react": "18.1.0",
    "react-i18next": "^11.18.6",
    "react-native": "0.70.2",
    "react-native-config": "^1.4.6",
    "react-native-image-crop-picker": "^0.38.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "^^2.2.4",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.0",
    "react-native-svg": "^13.2.0",
    "react-native-uuid": "^2.0.1",
    "react-redux": "^8.0.4",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@firebase/firestore-types": "^2.5.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^29.1.1",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.4",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-logger": "^3.0.9",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
